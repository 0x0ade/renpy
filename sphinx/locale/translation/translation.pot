# 899f04db4d6d429db38dee46c3ff8c09
msgid "Changes the language of the game to `language`."
msgstr ""

#: ../../source/inc/language_action:8
# 3b59ed972a0344d5a4dd70cf6c03dab7
msgid "`language`"
msgstr ""

#: ../../source/inc/language_action:8
# 38d4e9db5a8741ea8216473e6b4cd463
msgid "A string giving the language to translate to, or None to use the default language of the game script."
msgstr ""

#: ../../source/translation.rst:354
# 84fda7f75700457f8a00f29d5c0410d8
