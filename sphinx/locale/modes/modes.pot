msgstr ""

#: ../../source/modes.rst:63
# f61b2e0d8e564cfba2e395b05e2e8c70
msgid "imagemap"
msgstr ""

#: ../../source/modes.rst:62
# 33e57c96837b4c6997294f643290b0c2
msgid "The mode Ren'Py enters when an old-style imagemap is invoked using :func:`renpy.imagemap`."
msgstr ""

